{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "carsalesadfactory"
		},
		"AzureSqlDatabaseLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabaseLS'"
		},
		"lsb_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lsb_datalake'"
		},
		"AzureSqlDatabaseLS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "servercarsalesdata.database.windows.net"
		},
		"AzureSqlDatabaseLS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "carsalessqldb"
		},
		"AzureSqlDatabaseLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminprerak"
		},
		"githubLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"lsb_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://carsalesdata.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "carsalesadfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabaseLS_password": {
						"value": "[parameters('AzureSqlDatabaseLS_password')]"
					},
					"lsb_datalake_accountKey": {
						"value": "[parameters('lsb_datalake_accountKey')]"
					},
					"AzureSqlDatabaseLS_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaseLS_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaseLS_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaseLS_properties_typeProperties_database')]"
					},
					"AzureSqlDatabaseLS_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabaseLS_properties_typeProperties_userName')]"
					},
					"githubLS_properties_typeProperties_url": {
						"value": "[parameters('githubLS_properties_typeProperties_url')]"
					},
					"lsb_datalake_properties_typeProperties_url": {
						"value": "[parameters('lsb_datalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}