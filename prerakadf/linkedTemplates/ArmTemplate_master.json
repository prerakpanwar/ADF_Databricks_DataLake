{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "prerakadf"
		},
		"lsSQLdb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lsSQLdb'"
		},
		"ls_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalake'"
		},
		"lsSQLdb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "prerakserversql.database.windows.net"
		},
		"lsSQLdb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "prerakdatabase"
		},
		"lsSQLdb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminprerak"
		},
		"ls_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://preraksalesdatalake.dfs.core.windows.net/"
		},
		"lsgithub_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "prerakadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lsSQLdb_password": {
						"value": "[parameters('lsSQLdb_password')]"
					},
					"ls_datalake_accountKey": {
						"value": "[parameters('ls_datalake_accountKey')]"
					},
					"lsSQLdb_properties_typeProperties_server": {
						"value": "[parameters('lsSQLdb_properties_typeProperties_server')]"
					},
					"lsSQLdb_properties_typeProperties_database": {
						"value": "[parameters('lsSQLdb_properties_typeProperties_database')]"
					},
					"lsSQLdb_properties_typeProperties_userName": {
						"value": "[parameters('lsSQLdb_properties_typeProperties_userName')]"
					},
					"ls_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_properties_typeProperties_url')]"
					},
					"lsgithub_properties_typeProperties_url": {
						"value": "[parameters('lsgithub_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}